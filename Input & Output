#Hello World
print("Hello World")

#Hello World with tab
print("Hello World\tHello World")

#Hello World with new line
print("Hello World\nHello World")   

#Integer Input
a=int(input())
print(a)

#Float Input
a=float(input())
print('%.2f'%a)

#Character Input
a=input()
print(a)

#String Input
a=str(input())
print(a)

#Ascii Value
a=input()
print(ord(a))

#Character of Ascii Value
a=int(input())
print(chr(a))

